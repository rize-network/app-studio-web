{
  "name": "@app-studio/web",
  "version": "0.1.12",
  "main": "dist/index.js",
  "typings": "dist/components/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "app-studio": "^0.1.16",
    "core-js": "^3.33.2",
    "date-fns": "^2.30.0",
    "formik": "^2.2.9"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "tsdx build --entry ./src/components/index.tsx --tsconfig tsconfig.build.json",
    "postbuild": "npx tsc-alias -p tsconfig.build.json",
    "test": "react-scripts test",
    "test:unwatch": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "eslint": "eslint --cache --ext .ts,.tsx",
    "eslint:fix": "eslint --fix --cache --ext .ts,.tsx",
    "lint": "npm run eslint && npm run prettier",
    "lint-staged": "lint-staged",
    "lint-staged:eslint": "eslint --fix --cache --ext .ts,.tsx",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "docker": "npm run build && docker-compose up",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx, json}\"",
    "prettier:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx, json}\"",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "commit:lint": "npm run test:unwatch && npm run lint:fix",
    "lighthouse": "lighthouse http://localhost:3000 --view --budget-path=./budget.json",
    "bundle-analyzer": "node ./scripts/analyse.js",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "release": "release-it"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.18.6",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.14",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-named-exports-order": "^0.0.2",
    "babel-plugin-react-require": "^4.0.0",
    "brotli-webpack-plugin": "^1.1.0",
    "commitizen": "^4.3.0",
    "compression": "^1.7.4",
    "compression-webpack-plugin": "^10.0.0",
    "css-loader": "^6.7.3",
    "dotenv": "^16.1.4",
    "env-cmd": "^10.1.0",
    "eslint": "^8.41.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "express": "^4.18.2",
    "git-conventional-commits": "^1.4.2",
    "git-cz": "^4.9.0",
    "husky": "^8.0.0",
    "lighthouse": "^10.2.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-docgen-typescript": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.1",
    "react-scripts": "5.0.1",
    "react-styleguidist": "^13.1.1",
    "react-test-renderer": "^18.2.0",
    "release-it": "^16.2.1",
    "router": "^1.3.8",
    "style-loader": "^3.3.2",
    "styled-components": "^5.3.11",
    "ts-loader": "^9.4.2",
    "tsc-alias": "^1.8.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.1.3",
    "zustand": "^4.3.8"
  },
  "peerDependencies": {
    "react": ">= 16",
    "styled-components": ">= 5"
  },
  "size-limit": [
    {
      "path": "dist/web.cjs.production.min.js",
      "limit": "100 KB"
    },
    {
      "path": "dist/web.esm.js",
      "limit": "100 KB"
    }
  ],
  "module": "dist/web.esm.js"
}
