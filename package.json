{
  "name": "@app-studio/web",
  "version": "0.9.93",
  "main": "dist/web.cjs.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "docs"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "vite",
    "build:web": "vite build",
    "build": "vite build --config vite.lib.config.ts",
    "test": "vitest",
    "test:unwatch": "vitest run",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "eslint": "eslint --cache --ext .ts,.tsx",
    "eslint:fix": "eslint . --ext .ts,.tsx --fix",
    "lint": "npm run eslint && npm run prettier",
    "lint-staged": "lint-staged",
    "lint-staged:eslint": "eslint . --ext .ts,.tsx",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "docker": "npm run build && docker-compose up",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx, json}\"",
    "prettier:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx, json}\"",
    "prepare": "husky install",
    "commit:lint": "npm run lint:fix",
    "lighthouse": "lighthouse http://localhost:3000 --view --budget-path=./budget.json",
    "bundle-analyzer": "node ./scripts/analyse.js",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "prerelease": "npm run build",
    "release": "release-it",
    "bot-doc": "ts-node bot/index.ts",
    "create-docs": "ts-node scripts/create-docs.ts",
    "create-structure": "ts-node scripts/create-structure.ts",
    "create-indices": "ts-node scripts/create-indices.ts",
    "create-example": "ts-node scripts/create-example.ts",
    "page-imports": "ts-node scripts/generate-page-imports.ts",
    "create-pages": "npm run create-indices && ts-node scripts/create-pages.ts && npm run page-imports",
    "validate-docs": "node scripts/validate-docs.js",
    "docs:app-studio": "rm -rf docs/app-studio && mkdir -p docs/app-studio && cp -R node_modules/app-studio/docs/* docs/app-studio",
    "api:local": "react-api  --useUnionTypes --input http://localhost:3000/docs/swagger.json --output ./src/services/api  && prettier --write ./src/services/api"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@app-studio/react-api": "^0.15.28",
    "@app-studio/react-request": "^0.1.3",
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@babel/preset-typescript": "^7.26.0",
    "@babel/standalone": "^7.27.3",
    "@commitlint/cli": "^19.2.0",
    "@commitlint/config-conventional": "^19.1.0",
    "@google/generative-ai": "^0.24.0",
    "@huggingface/inference": "^3.6.2",
    "@mdx-js/mdx": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@mdxeditor/editor": "^3.21.0",
    "@size-limit/preset-small-lib": "^10.0.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "@vitejs/plugin-react": "^4.7.0",
    "date-fns": "^2.30.0",
    "app-studio": "^0.7.7",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-named-exports-order": "^0.0.2",
    "babel-plugin-react-require": "^4.0.0",
    "cheerio": "^1.0.0-rc.12",
    "commitlint": "^18.2.0",
    "css-loader": "^6.7.4",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^8.0.0",
    "jsdom": "^27.4.0",
    "lighthouse": "^11.6.0",
    "lint-staged": "^13.2.2",
    "lucide-react": "^0.562.0",
    "formik": "^2.4.6",
    "typescript": "^4.9.5",
    "zustand": "^4.3.8",
    "openai": "^4.91.1",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.11.1",
    "react-test-renderer": "^18.2.0",
    "release-it": "^16.2.1",
    "replicate": "^1.0.1",
    "simple-git": "^3.22.0",
    "store": "^2.0.12",
    "tiktoken": "^1.0.20",
    "ts-loader": "^9.4.2",
    "tsc-alias": "^1.8.8",
    "tslib": "^2.8.1",
    "vite": "^5.4.21",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.18",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "yup": "^1.5.0"
  },
  "peerDependencies": {
    "lucide-react": "*",
    "react": ">= 17",
    "react-dom": ">= 17",
    "react-router-dom": ">= 6",
    "app-studio": ">= 0.7",
    "typescript": ">=4",
    "zustand": ">=4",
    "formik": ">=2"
  },
  "size-limit": [
    {
      "path": "dist/web.cjs.js",
      "limit": "100 KB"
    },
    {
      "path": "dist/web.esm.js",
      "limit": "100 KB"
    },
    {
      "path": "dist/web.umd.js",
      "limit": "100 KB"
    }
  ],
  "module": "dist/web.esm.js",
  "unpkg": "dist/web.umd.js"
}
