{
  "name": "@app-studio/web",
  "version": "0.8.66",
  "main": "dist/index.js",
  "typings": "dist/components/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "react-scripts start",
    "build:web": "react-scripts build",
    "build": "tsdx build --format cjs,esm,umd --entry ./src/components/index.tsx --tsconfig tsconfig.build.json",
    "test": "react-scripts test",
    "test:unwatch": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "eslint": "eslint --cache --ext .ts,.tsx",
    "eslint:fix": "eslint --fix --cache --ext .ts,.tsx",
    "lint": "npm run eslint && npm run prettier",
    "lint-staged": "lint-staged",
    "lint-staged:eslint": "eslint --fix --cache --ext .ts,.tsx",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "docker": "npm run build && docker-compose up",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx, json}\"",
    "prettier:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx, json}\"",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "commit:lint": "npm run lint:fix",
    "lighthouse": "lighthouse http://localhost:3000 --view --budget-path=./budget.json",
    "bundle-analyzer": "node ./scripts/analyse.js",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "prerelease": "npm run build",
    "release": "release-it",
    "bot-doc": "ts-node bot/index.ts",
    "create-docs": "ts-node scripts/create-docs.ts",
    "create-structure": "ts-node scripts/create-structure.ts",
    "create-indices": "ts-node scripts/create-indices.ts",
    "create-example": "ts-node scripts/create-example.ts",
    "page-imports": "ts-node scripts/generate-page-imports.ts",
    "create-pages": "npm run create-indices && ts-node scripts/create-pages.ts && npm run page-imports"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "contrast": "^1.0.1",
    "date-fns": "^2.30.0",
    "formik": "^2.4.6",
    "typescript": "^4.9.5",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@babel/preset-typescript": "^7.26.0",
    "@commitlint/cli": "^19.2.0",
    "@commitlint/config-conventional": "^19.1.0",
    "@google/generative-ai": "^0.24.0",
    "@huggingface/inference": "^3.6.2",
    "@mdx-js/mdx": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@mdxeditor/editor": "^3.21.0",
    "@size-limit/preset-small-lib": "^10.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.14",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "app-studio": "^0.5.83",
    "async-mutex": "^0.5.0",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-named-exports-order": "^0.0.2",
    "babel-plugin-react-require": "^4.0.0",
    "brotli-webpack-plugin": "^1.1.0",
    "cheerio": "^1.0.0-rc.12",
    "commitlint": "^18.2.0",
    "compression": "^1.7.4",
    "compression-webpack-plugin": "^10.0.0",
    "css-loader": "^6.7.4",
    "dotenv": "^16.4.5",
    "env-cmd": "^10.1.0",
    "eslint": "^8.41.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "express": "^4.18.2",
    "groq-sdk": "^0.17.0",
    "husky": "^8.0.0",
    "lighthouse": "^11.6.0",
    "lint-staged": "^13.2.2",
    "markdown-loader": "^8.0.0",
    "npm": "^10.5.0",
    "openai": "^4.91.1",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-docgen-typescript": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-live": "^4.1.7",
    "react-router-dom": "^6.11.1",
    "react-scripts": "^5.0.1",
    "react-styleguidist": "^13.1.1",
    "react-test-renderer": "^18.2.0",
    "release-it": "^16.2.1",
    "replicate": "^1.0.1",
    "router": "^1.3.8",
    "simple-git": "^3.22.0",
    "style-loader": "^3.3.2",
    "tiktoken": "^1.0.20",
    "ts-loader": "^9.4.2",
    "tsc-alias": "^1.8.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "web-vitals": "^2.1.4",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.1.3",
    "yup": "^1.5.0"
  },
  "peerDependencies": {
    "react": ">= 17"
  },
  "size-limit": [
    {
      "path": "dist/web.cjs.production.min.js",
      "limit": "100 KB"
    },
    {
      "path": "dist/web.esm.js",
      "limit": "100 KB"
    },
    {
      "path": "dist/web.umd.production.min.js",
      "limit": "100 KB"
    }
  ],
  "module": "dist/web.esm.js",
  "unpkg": "dist/web.umd.production.min.js"
}
